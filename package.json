{
  "name": "react-dashboard",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "description": "A React dashboard template with vite, tailwindcss, zustand, react-hook-form, react-router-dom, framer-motion, shadcn/ui, @heroicons/react",
  "scripts": {
    "dev": "vite",
    "build": "tsc && vite build",
    "lint": "eslint . --ext ts,tsx --report-unused-disable-directives --max-warnings 0",
