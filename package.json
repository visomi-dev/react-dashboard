    "eslint-plugin-react-refresh": "^0.4.3",
    "husky": "^8.0.3",
    "postcss": "^8.4.29",
    "prettier": "^3.0.3",
    "prettier-plugin-tailwindcss": "^0.5.4",
    "tailwind-merge": "^1.14.0",
    "tailwindcss": "^3.3.3",
